{% if(Address) %}
    {{ Address.getStreetName }}
    {{ Address.getStreetNumber }}
    {{ Address.getPostOfficeBox }}
    <br/>
    {% set City = Address.getTblCity %}
    {% if(City) %}
        {{ City.getCode }}
        {{ City.getName }}
        {{ City.getDistrict }}
    {% endif %}
    <br/>
    {% set State = Address.getTblState %}
    {% if(State) %}
        {{ State.getName }}
    {% endif %}
{% endif %}
